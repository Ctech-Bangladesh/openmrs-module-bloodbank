<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
  package="org.openmrs.module.bloodbank.api.model">
  <class name="BloodAcceptor" table="bloodbank_acceptor" lazy="false" batch-size="50">
    <id name="acceptorId" type="java.lang.Integer" column="acceptor_id">
      <generator class="native">
        <param name="sequence">acceptor_id_seq</param>
      </generator>
    </id>
    <property name="acceptorName" type="java.lang.String" column="acceptor_name"/>

  </class>

</hibernate-mapping>
